    "from lets_plot import *\n",
    "LetsPlot.setup_html()\n",
    "\n",
    "n = 5\n",
    "x = [i * 10**(-5) for i in range(n)]\n",
    "y = [0] * len(x)\n",
    "\n",
    "ggplot(dict(x=x, y=y), aes('x', 'y')) + \\\n",
    "    geom_point() + \\\n",
    "    scale_x_continuous(format='.7e')"
